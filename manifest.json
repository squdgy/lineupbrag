{
  "manifest_version": 2,

  "name": "Lineup Brag",
  "description": "This extension helps you share your incredible fantasy lineups",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title":  "Brag It!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}